set fileencodings=utf-8,cp936,gb18030,big5,euc-jp,euc-kr,latin1
set lines=50 columns=160

set foldenable
set foldmethod=indent
set foldlevelstart=99
nnoremap @=((foldclosed(line('.')) < 0) ? 'zc' : 'zo')

set linespace=1
set transparency=1
set macligatures
set antialias

set termguicolors
let ayucolor="mirage"
colorscheme onedark

set guifont=Menlo:h14
" set cursorline


